-- Create user for PROGRAMMATIC access. MFA Enabled user eg John will not work
create user interop_user;
alter user set default_role = accountadmin;
alter user set default_warehouse = snow_intelligence_demo_wh ;
ALTER USER interop_user SET RSA_PUBLIC_KEY='';
create warehouse interopdemo_setup_wh;